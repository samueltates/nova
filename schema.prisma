// generator
generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

datasource db {
  provider = "sqlite"
  url      = "file:database.db"
}

model log {
  id        Int    @id @default(autoincrement())
  SessionID String
  UserID    String
  date      String
  summary   String
  body      String
}

model message {
  id        Int    @id @default(autoincrement())
  SessionID String
  UserID    String
  name      String
  timestamp String
  body      String
}
